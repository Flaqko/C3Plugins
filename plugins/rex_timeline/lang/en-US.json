					"triggeredtimername": {
						"description": "Get triggered timer's name.",
						"translated-name": "TriggeredTimerName"
						},
					"timerdelaytime": {
						"description": "Get delay time of timer.",
						"translated-name": "TimerDelayTime",
						"params": {
							"name0": { "name":"Name", "desc":"Timer's name."}
